#include <stdio.h>
#include <stdint.h>

void myFunction(char c) {
    if (c >= 0 && c <= UINT32_MAX) {
        uint32_t value = (uint32_t)c;
        printf("The value of the char argument is: %u\n", value);
    } else {
        printf("The char argument is out of range for a uint32_t value\n");
    }
}


int main(char x){
	
	myFunction(x);
	return 0;
}
